<!-- Fonts -->
<link
    rel="preload"
    href="/fonts/PublicSans-Regular.woff2"
    as="font"
    type="font/woff2"
    crossorigin="true"
>
<link
    rel="preload"
    href="/fonts/PublicSans-Italic.woff2"
    as="font"
    type="font/woff2"
    crossorigin="true"
>
